{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "YaacDbContext": "Filename=yaac.db"
  },
  "IdentityOptions": {
    "Password": {
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": true
    },
    "Lockout": {
      "MaxFailedAccessAttempts": 3,
      "DefaultLockoutTimeSpan": "00:03:00"
    },
    "User": {
      "RequireUniqueEmail": false
    }
  },
  "AuthOptions": {
    "TokenKey": "Yet Another AI Chat Jwt Token SecretKey"
  }
}
